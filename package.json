{
  "name": "boilerplate-websocket",
  "version": "1.0.0",
  "description": "Boilerplate para desenvolvimento de Websocket.",
  "keywords": [
    "webhook",
    "websocket",
    "TELEPORT"
  ],
  "main": "server/index.js",
  "type": "module",
  "author": "Thiago Lagden",
  "license": "MIT",
  "scripts": {
    "eslint": "eslint --config .eslintrc.yml --ext .js --ignore-path .gitignore .",
    "pretest": "npm run eslint",
    "test": "c8 --reporter=text --reporter=text-summary --reporter=lcov ava --tap --verbose -T 60000",
    "prestart": "npm run eslint",
    "start": "node server"
  },
  "dependencies": {
    "@tadashi/jwt": "8.5.0",
    "@tadashi/local-access": "1.0.1",
    "cookie": "0.5.0",
    "debug": "4.3.4",
    "uWebSockets.js": "https://github.com/uNetworking/uWebSockets.js/archive/v20.14.0.tar.gz"
  },
  "devDependencies": {
    "@tadashi/hex-id": "2.0.0",
    "ava": "5.0.1",
    "c8": "7.12.0",
    "eslint": "8.26.0",
    "eslint-config-xo": "0.42.0",
    "eslint-plugin-unicorn": "44.0.2",
    "hash-to-port": "1.2.0",
    "p-event": "5.0.1",
    "ws": "8.10.0"
  }
}
